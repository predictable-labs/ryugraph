-STATEMENT MATCH (a)-[t:LivesWith]->(d) WHERE a.name <> d.name WITH a, d, t MATCH p = (a)-[:LivesWith*1..]->(d) WHERE length(p) >= 2 AND ALL (r IN relationships(p) WHERE r <> t) WITH DISTINCT t RETURN count(t);
